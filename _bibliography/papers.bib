@article{Fuchs-Hanusch2015,
abstract = {In diesem Beitrag wurde eine Methode zur optimalen Platzierung von Sensoren unter Ber{\"{u}}cksichtigung von Verbraucherunsicherheiten vorgestellt. Der Test der vorgestellten Methodik zur optimalen Sensorplatzierung und modellbasierten Leckageeingrenzung Hess trotz grundlegender Unterschiede der gew{\"{a}}hlten hydraulischen Testnetze dieselben R{\"{u}}ckschl{\"{u}}sse zu. Die Anwendungsbeispiele zeigten deutlich, dass Netzbereiche, die hochsensitiv auf das Auftreten von Leckagen reagieren, gleichzeitig auch jene Bereiche sein k{\"{o}}nnen, in denen Verbrauchsunsicherheiten zu hohen Unsicherheiten des Druckes f{\"{u}}hren. Aus diesem Grund sollten diese Unsicherheiten bereits bei der Ermittlung der optimalen Sensorposition Ber{\"{u}}cksichtigung finden, um Positionen auszuschliessen, in welchen zu erwartende Schwankungen des Verbrauches zu einer Verschleierung allf{\"{a}}lliger Leckagen f{\"{u}}hren k{\"{o}}nnen. Die modellbasierte Leckageeingrenzung wird im n{\"{a}}chsten Schritt in der Praxis getestet. Im August 2015 wurden dazu Drucksensoren an den vorgeschlagenen Positionen installiert. Dabei kamen neben den genannten Aspekten auch Aspekte der Stromversorgung und Sicherstellung einer stabilen Daten{\"{u}}bertragung hinzu. Diese Randbedingungen wurden bei der Auswahl der realen Standorte f{\"{u}}r den Einbau ebenfalls ber{\"{u}}cksichtigt. F{\"{u}}r tempor{\"{a}}re Messungen, in den frostfreien Monaten, kommen z B Hydranten als Position f{\"{u}}r die Druckmessung infrage. Die Information zur Lage von Hydranten im Netz wurde dabei abschliessend ebenfalls in die Methodik zur Sensorplatzierung einbezogen, wodurch sich in der Praxis den idealen Netzknoten nahe gelegene Hydranten als optimale Messpunkte ergaben. Da die L{\"{o}}sung des inversen Problems zur Leckageeingrenzung den Ans{\"{a}}tzen zur modellbasierten Netzkalibrierung sehr {\"{a}}hnlich ist, kann der entwickelte Algorithmus durch weitere Adaptierungen auch zur Definition geeigneter Messpunkte zum Zweck der Netzkalibrierung angewendet werden Auch hierzu laufen am Institut bereits Folgeuntersuchungen.},
author = {Fuchs-Hanusch, Daniela and Steffelbauer, David B. and G{\"{u}}nther, Markus},
journal = {Aqua {\&} Gas},
number = {11},
pages = {26--31},
publisher = {SVGW},
title = {{Drucksensoren zur Leckagelokalisierung. Optimale Platzierung der Sensoren durch Ber{\"{u}}cksichtigung von Verbraucherunsicherheiten}},
url = {https://www.tib.eu/de/suchen/id/tema{\%}3ATEMA20151101880/Drucksensoren-zur-Leckagelokalisierung-Optimale/},
volume = {95},
year = {2015}
}
@article{Gunther2015,
abstract = {Nowadays, modern utilities have installed a variety of measurement devices like pressure sensors, flow meters, noise loggers, water quality sensors for different intentions. In this paper we introduce an experimental water distribution system (EWDS-TUG) that serves as an example of implementing a smart water network solution. The used EWDS-TUG states a highly determined water supply system. Artificial customers are installed and can be controlled automatically by magnetic valves. We present how we collect sensor data and control magnetic valves installed, remotely via a web based control and communication solution, which is deeply integrated in the measurement workflow.},
